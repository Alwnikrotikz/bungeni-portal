<div class="listing">
  <h3> Parliament </h3>
  <table class="listing">
    <thead>
      <tr>
        <th> Name </th>
        <th> Start date</th>
        <th> End Date</th>                
      </tr>
    </thead>
    <tbody>
      <tr tal:repeat="data view/getData"> 
        <td > <a tal:attributes="href data/url" tal:content="data/short_name"> X Parliament</a> 
        <td tal:content="data/start_date" />
        <td tal:content="data/end_date" />
      </tr>
    </tbody>  
</div>
