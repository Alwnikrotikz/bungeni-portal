<div class="listing">
  <h3> Committees </h3>
  <table class="listing">
    <thead>
      <tr>
        <th> Short name </th>
        <th> Name </th>        
        <th> Start date</th>
        <th> End Date</th>                
      </tr>
    </thead>
    <tbody>
      <tr tal:repeat="data view/getData"> 
        <td> <a tal:attributes="href data/url" tal:content="data/short_name"/> </td>
        <td> <a tal:attributes="href data/url" tal:content="data/full_name"/>  </td>      
        <td tal:content="data/start_date" />
        <td tal:content="data/end_date" />
      </tr>
    </tbody>  
  </table>
</div>
