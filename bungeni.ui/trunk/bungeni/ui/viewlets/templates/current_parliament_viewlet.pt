<div>
    <!--h3 tal:define="data view/getCurrentData" tal:content="data/short_name"> Current Parliament </h3-->
    <h3> Current Parliament </h3>
    <table class="listing">
      <thead>
        <th> Name </th>
        <th> Election date</th>
        <th> In power from</th>
        <th> In power till</th>    
      </thead>
      <tbody tal:define="data view/getCurrentData">            
        <td> <a tal:attributes="href data/url" tal:content="data/short_name"> X Parliament</a> </td>
        <td> <a tal:attributes="href data/url" tal:content="data/election_date" /></td>
        <td> <a tal:attributes="href data/url" tal:content="data/start_date" /></td>
        <td> <a tal:attributes="href data/url" tal:content="data/end_date" /></td>
      </tbody>
    </table>

    <div class="listing">
      <em> Choose a Parliament </em>
      <table class="listing">
        <thead>
          <tr>
            <th/>
            <th> Name </th>
            <th> Election date</th>
            <th> In power from</th>
            <th> In power till</th>              
            <!--th> Members of Parliament</th-->      
          </tr>
        </thead>
        <tbody>
          <tr tal:repeat="data view/getData"> 
            <td tal:attributes="class data/current" tal:content="data/selector" />
            <td tal:attributes="class data/current"> <a tal:attributes="href data/url" tal:content="data/short_name"> X Parliament</a> </td>
            <td tal:attributes="class data/current"> <a tal:attributes="href data/url" tal:content="data/election_date" /> </td>
            <td tal:attributes="class data/current"> <a tal:attributes="href data/url" tal:content="data/start_date" /> </td>
            <td tal:attributes="class data/current"> <a tal:attributes="href data/url" tal:content="data/end_date" /> </td>
            <!--td tal:attributes="class data/current"> <a tal:attributes="href data/mpurl"> Members of Parliament </a> </td-->
          </tr>
        </tbody>  
      </table>
    </div>
</div>

