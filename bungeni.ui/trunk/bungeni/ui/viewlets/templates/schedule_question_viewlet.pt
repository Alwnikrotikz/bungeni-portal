<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      i18n_domain="bungeni.ui">

  <div class="workspace-viewlet"
       tal:define="results view/getData"
       tal:attributes="id string:workspace-viewlet-${view/name}">
    <h4 i18n:translate="" tal:content="view/name" />
  
    <ul tal:condition="results" tal:attributes="id view/list_id; class string:draglist">
      <li tal:repeat="result results" tal:attributes="id result/qid" tal:content="result/subject">
       
      </li>
    </ul>

    <p tal:condition="not: results" i18n:translate="">
      No items.
    </p>
  </div> 
</html>
