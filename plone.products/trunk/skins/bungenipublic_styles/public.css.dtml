a span.hovertooltip {
  display: none;
  }
 
a:hover {
  background-color: #EEEEEE;
  }
/* The above hover change defeats the
  IE/Win display changing bug */
 
a:hover span.hovertooltip {
  position: absolute;
  display: block;
  background: #f4e0b5;
