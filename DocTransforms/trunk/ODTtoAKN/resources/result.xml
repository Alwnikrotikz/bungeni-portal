      <eol/>
      <eol/>
      <eol/>
      <subdivision>
         <p xmlns:text="urn:oasis:names:tc:opendocument:xmlns:text:1.0" text:style-name="P7">ADJOURNMENT</p>
      </subdivision>
      <eol/>
