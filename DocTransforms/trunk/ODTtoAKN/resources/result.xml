            <Communication type="OralAnswersToQuestions" id="" by="" to="" status="">
               <p xmlns:text="urn:oasis:names:tc:opendocument:xmlns:text:1.0" text:style-name="P15">
                  <ref class="BungeniSpeechAs:8">
                  <span text:style-name="T2">Mr. Gatabaki</span>
                  </ref>
                  <span text:style-name="T1">:</span>
               </p>
            </Communication>
            <eol/>
         </Communication>
         <Communication type="OralAnswersToQuestions" id="" by="" to="" status="">
            <p xmlns:text="urn:oasis:names:tc:opendocument:xmlns:text:1.0" text:style-name="P6"/>
            <Communication type="OralAnswersToQuestions" id="" by="" to="" status="">
               <p xmlns:text="urn:oasis:names:tc:opendocument:xmlns:text:1.0" text:style-name="P15">
                  <ref class="BungeniSpeechAs:9">
                  <span text:style-name="T2">Mr. Speaker</span>
                  </ref>
                  <span text:style-name="T1">:</span>
               </p>
            </Communication>
            <eol/>
            <p xmlns:text="urn:oasis:names:tc:opendocument:xmlns:text:1.0" text:style-name="P1">Next Question, Dr. Kulundu!</p>
         </Communication>
         <eol/>
         <eol/>
